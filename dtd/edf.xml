        <!--liste des personnages qui interviennent dans l'action hors scène-->
        <ref>
            <span from="l1626" to="l1641"/>
            <!--référence des vers qui mentionnent l'action hors scène-->
        </ref>
    </event>
    <event xml:id="HS2" from="#HS1" to="#V07">
        <stage who="#oronte #enrique #chrysalde"/>
        <ref>
            <span from="l1652"/>
        </ref>
    </event>
</listEvent>
